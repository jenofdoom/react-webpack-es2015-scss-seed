{
  "name": "react-webpack-es2015-scss-seed",
  "version": "1.0.0",
  "description": "A project seed using React, Webpack, Babel + ES2015 and scss",
  "repository": "https://github.com/jenofdoom/react-webpack-es2015-scss-seed",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
